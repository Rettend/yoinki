{
    "manifest_version": 3,
    "name": "Yoinki",
    "description": "Create Anki cards with ChatGPT",
    "version": "0.0.1",
    "icons": {
        "16": "icon-16.png",
        "32": "icon-32.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "action": {
        "default_popup": "/src/popup/popup.html"
    },
    "background": {
        "service_worker": "/src/background/background.js"
    },
    "permissions": [
        "tabGroups"
    ],
    "host_permissions": [
        "https://developer.chrome.com/*"
    ]
}